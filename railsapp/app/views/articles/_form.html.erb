<%= form_with model: @article, data: { turbo: false } do |form| %>
    <div>
        <%= form.label :name, "ウェブサイト名", style: "display: block"%>
        <%= form.text_field :name%>
    </div>

    <div>
        <%= form.label :url, "URL", style: "display: block" %>
        <%= form.text_field :url%>
    </div>

    <div>
        <%= form.label :category_ids, "カテゴリ", style: "display: block" %>
        <%= form.collection_check_boxes :category_ids, Category.all, :id, :name %>
    </div>

    <div>
        <%= form.submit %>
    </div>
<% end %>